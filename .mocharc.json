{
  "node-option": [
    "import=tsimp/import"
  ],
  "ignore": [
    "/build/",
    "/node_modules/",
    "./build/",
    "./node_modules/"
  ],
  "timeout": 100,
  "extension": [
    "test.ts"
  ],
  "bail": false,
  "package": "./package.json",
  "fullTrace": false,
  "ui": "bdd",
  "color": true,
  "recursive": true,
  "reporter": "spec",
  "slow": 100,
  "watch": false,
  "retries": 0,
  "exit": true
}
