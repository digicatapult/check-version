{
  "name": "@digicatapult/check-version",
  "version": "1.4.97",
  "description": "Asserts package version is the same or higher than latest published tag",
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "depcheck": "depcheck",
    "check": "tsc",
    "build": "ncc build  --license licenses.txt ./src/main.ts && mv ./dist/index.js ./dist/index.mjs",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "mocha --config .mocharc.json '{,!(node_modules)/**/}*.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digicatapult/check-version.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "semver": "^7.7.1",
    "toml": "^3.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@tsconfig/node12": "^12.1.4",
    "@types/chai": "^5.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.24",
    "@types/semver": "^7.5.8",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vercel/ncc": "^0.38.3",
    "chai": "^5.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-github": "^5.1.8",
    "mocha": "^11.1.0",
    "prettier": "^3.5.3",
    "sinon": "^19.0.2",
    "@digicatapult/tsimp": "^2.0.12",
    "typescript": "^5.8.2"
  }
}
