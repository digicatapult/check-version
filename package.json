{
  "name": "@digicatapult/check-version",
  "version": "1.1.27",
  "description": "Asserts package version is the same or higher than latest published tag",
  "main": "dist/index.js",
  "scripts": {
    "depcheck": "depcheck",
    "build": "ncc build  --license licenses.txt ./src/main.ts ",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "mocha --config .mocharc.js '{,!(node_modules)/**/}*.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digicatapult/check-version.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "semver": "^7.5.4",
    "toml": "^3.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@tsconfig/node12": "^12.1.0",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.4",
    "@types/semver": "^7.5.2",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vercel/ncc": "^0.38.0",
    "chai": "^4.3.8",
    "depcheck": "^1.4.6",
    "eslint": "^8.50.0",
    "eslint-plugin-github": "^4.10.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "sinon": "^16.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
